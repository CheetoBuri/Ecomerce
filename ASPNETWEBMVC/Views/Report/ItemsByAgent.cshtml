@model IEnumerable<dynamic>


@{
    ViewBag.Title = "Items Purchased by Agent";
}

<h2>Items Purchased by @ViewBag.AgentName</h2>

<table class="table">
    <thead>
        <tr>
            <th>Item Name</th>
            <th>Total Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ItemName</td>
                <td>@item.TotalQuantity</td>
            </tr>
        }
    </tbody>
</table>


<p>@Html.ActionLink("Back to Reports", "Index")</p>
